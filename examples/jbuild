(jbuild_version 1)

(executables
  ((names (linear_regression confidence_interval))
   (libraries (decml containers))
   (preprocess (pps (decml.ppx)))
  ))

(alias 
  ((name linear_regression_demo)
   (deps (linear_regression.exe))
   (action  (run ${<} -data-file ../../../examples/data/cats_data.csv))))

(alias 
  ((name confidence_interval_demo)
   (deps (confidence_interval.exe))
   (action  (run ${<} -data-file ../../../examples/data/cats_data.csv))))